{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "node server.js",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE"
  },
  "variables": {
    "UPSTREAM_URL": "",
    "UPSTREAM_AUTH": "",
    "CACHE_TTL": "60",
    "REDIS_URL": "",
    "CORS_ORIGIN": "*",
    "PORT": "8080"
  }
}